<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.limin.mapper.NavMapper">

    <select id="queryNavList" resultType="Nav">
        select * from nav
    </select>

    <insert id="createNav" parameterType="Nav">
        insert into nav (title,navSrc,navUrl,createDate) values (#{title},#{navSrc},#{navUrl},#{createDate})
    </insert>

</mapper>