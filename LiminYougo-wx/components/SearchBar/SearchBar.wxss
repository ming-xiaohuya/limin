/* components/SearchBar/SearchBar.wxss */
.navigation-container {
    position: fixed;
    width: 100%;
    z-index: 99;
    top: 0;
    left: 0;
    background-color: #ffffff;
}
.navigation-bar {
    display: flex;
    flex-direction: row;
    align-items: center;
}
.search-row {
    position: absolute;
    width: 58%;
    left: 10%;
    bottom: 7%;
    font-size: 32rpx;
    border-radius: 35rpx;
    background-color: #f2f2f2;
    padding: 10rpx 22rpx;
}
input {
    position: relative;
    width: 60%;
    color: #bebebe;
}
.search-row .search-btn {
    position: absolute;
    top: 0;
    right: 10rpx;
    color: #ffffff;
    background-color: #fb2c02;
    border-radius: 30rpx;
    font-size: 26rpx;
    font-weight: normal;
    margin-top: 10rpx;
    padding: 10rpx 20rpx;
}
