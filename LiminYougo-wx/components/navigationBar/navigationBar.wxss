/* components/navigationBar/navigationBar.wxss */
.navigation-container {
    position: fixed;
    width: 100%;
    z-index: 99;
    top: 0;
    left: 0;
    background-color: #ffffff;
}
.navigation-bar {
    position: relative;
    display: flex;
    flex-direction: row;
    align-items: center;
}
.navigation-container iamge{
    width: 100%;
}
.navigation-title {
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    margin-top: 5%;
    text-align: center;
    font-size: 16px;
    font-weight: bold;
    color: black;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}